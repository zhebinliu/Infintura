  - orgshape
  - utilities
  - core-objects-duplicate-management
  - cl-approval-authority
  - cl-collaterals
  - cl-collection-configuration
  - cl-dag
  - cl-departments
  - cl-integration-framework
  - cl-origination-configuration
  - cl-rendered-fields
  - cl-rules
  - cl-servicing-stage-1
  - cl-servicing-stage-2
  - conga-doc-config
